{
  "name": "vinyl-shelves",
  "version": "0.1.0",
  "description": "Displays Discogs user's record collections visually",
  "main": "server/server.js",
  "scripts": {
    "knex": "knex",
    "test": "tape 'tests/**/*.test.js' | tap-spec",
    "dev": "node ./server ./src/ ./webpack.dev.config",
